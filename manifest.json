{
  "manifest_version": 2,

  "name": "Exercise with Clicks",
  "description": "Literally the greatest chrome extension to ever exist.",
  "version": "0.0.0.1",
  "options_page": "options.html",

  "permissions":[
    "storage",
    "notifications"
  ],

  "web_accessible_resources": ["stuff.js", "img/trophy.png"],

  "content_scripts": [{
    "js": ["jquery.min.js","clickdb.js","stuff.js"],
    "matches": [ "http://*/*", "https://*/*" ]
  }],

  "background": {
    "scripts": ["clickdb.js","background.js"],
    "persistent": false
  },

  "browser_action": {

  }
}